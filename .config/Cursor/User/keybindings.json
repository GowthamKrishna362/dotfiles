[
  {
    "key": "alt+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "alt+l",
    // move right
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "alt+x",
    "command": "workbench.action.closeEditorsInGroup"
  },
  {
    "key": "alt+x",
    "command": "workbench.action.closeSidebar",
    "when": "sideBarVisible && !editorTextFocus && !panelFocus && !chatPanelFocus"
  },
  {
    "key": "alt+x",
    "command": "composer.closeComposerTab",
    "when": "composerFocused && focusedView =~ /^workbench.panel.aichat.view/"
  },
  {
    "key": "alt+x",
    "command": "git.openFile",
    "when": "isInDiffEditor"
  },
  {
    "key": "ctrl+b",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "space f b",
    "command": "workbench.action.showAllEditors",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space r n",
    "command": "editor.action.rename",
    "when": "vim.mode == 'Normal' && editorHasRenameProvider && editorTextFocus && !editorReadOnly"
  },
  {
    "key": "space f s",
    "command": "workbench.action.findInFiles",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space s m",
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space s e",
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "space space",
    "command": "workbench.action.quickOpen",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  // {
  //   "key": "-",
  //   "command": "workbench.view.explorer",
  //   "when": "vim.mode == 'Normal' && editorTextFocus"
  // },
  {
    "key": "space s v",
    "command": "workbench.action.splitEditorRight",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "key": "ctrl+i",
    "command": "composerMode.agent"
  },
  {
    "key": "ctrl+o",
    "command": "search.action.openNewEditorToSide"
  },
  {
    "key": "ctrl+j",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "down",
    "command": "-list.focusDown",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "ctrl+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious"
  },
  {
    "key": "ctrl+e",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "-",
    "command": "voil.openPanelCurrentDir",
    "when": "!voilDoc && vim.mode == 'Normal' && editorFocus"
  },
  {
    "key": "-",
    "command": "voil.gotoParentDir",
    "when": "voilDoc && vim.mode == 'Normal' && editorFocus"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "-workbench.action.quickOpenNavigatePreviousInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "alt+tab",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "alt+tab",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!inEditorsPicker"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  }
]
